<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/boton3"
        android:layout_width="191dp"
        android:layout_height="84dp"
        android:contentDescription="boton3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.406"
        app:srcCompat="@drawable/logo_ilerna" />

    <ImageButton
        android:id="@+id/boton4"
        android:layout_width="191dp"
        android:layout_height="84dp"
        android:layout_marginBottom="280dp"
        android:contentDescription="boton4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton3"
        app:layout_constraintVertical_bias="0.087"
        app:srcCompat="@drawable/logo_ilerna" />

    <ImageButton
        android:id="@+id/boton5"
        android:layout_width="191dp"
        android:layout_height="84dp"
        android:layout_marginBottom="172dp"
        android:contentDescription="boton5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton4"
        app:layout_constraintVertical_bias="0.142"
        app:srcCompat="@drawable/logo_ilerna" />

    <ImageButton
        android:id="@+id/boton2"
        android:layout_width="191dp"
        android:layout_height="84dp"
        android:layout_marginTop="24dp"
        android:contentDescription="boton2"
        app:layout_constraintBottom_toTopOf="@+id/boton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton1"
        app:layout_constraintVertical_bias="0.422"
        app:srcCompat="@drawable/logo_ilerna" />

    <ImageButton
        android:id="@+id/boton1"
        android:layout_width="191dp"
        android:layout_height="84dp"
        android:contentDescription="boton1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        app:srcCompat="@drawable/logo_ilerna" />
</androidx.constraintlayout.widget.ConstraintLayout>